{
  "name": "react-webpack-node",
  "version": "1.5.3",
  "description": "A simple Node.js app using Express 4 with Webpack, React, alt, ImmutableJS",
  "repository": "https://github.com/choonkending/react-webpack-node",
  "main": "index.js",
  "scripts": {
    "babel": "babel --presets es2015,stage-0 --ignore node_modules,public",
    "babel-node": "babel-node --presets es2015,stage-0 --ignore node_modules,public",
    "lint": "eslint app",
    "clean": "rimraf public && rimraf compiled",
    "start": "cross-env NODE_ENV=production NODE_PATH=./server/middlewares node compiled/index.js",
    "dev": "cross-env NODE_ENV=development NODE_PATH=./server/middlewares nodemon",
    "build:dev": "cross-env NODE_ENV=development NODE_PATH=./server/middlewares webpack --colors --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "cross-env NODE_ENV=production NODE_PATH=./server/middlewares webpack --colors --config ./webpack/webpack.config.prod.js",
    "build:node": "cross-env NODE_ENV=production npm run babel -- ./server -d ./compiled",
    "build": "npm run clean && npm run build:prod && npm run build:node",
    "test": "cross-env NODE_ENV=test NODE_PATH=./server/middlewares karma start",
    "test:watch": "cross-env NODE_ENV=test NODE_PATH=./server/middlewares npm test -- --watch --no-single-run",
    "postinstall": "npm run build"
  },
  "author": "Choon Ken Ding",
  "license": "MIT",
